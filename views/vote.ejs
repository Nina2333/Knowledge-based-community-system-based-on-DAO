    <div class="d-flex align-items-center p-3 my-3 text-white bg-purple rounded shadow-sm">
        <img class="me-3" src="/images/3.svg" alt="" width="48" height="38">
        <div class="lh-1">
            <h1 class="h6 mb-0 text-white lh-1">基于 DAO 的知识型社区</h1>
            <small> </small>
        </div>
    </div>

    <nav class="navbar navbar-expand-lg ">
        <div class="container-fluid">
            <a class="navbar-brand" href="/vote">决议列表</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                    data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="/vote/create">创建议题</a>
                    </li>
                </ul>
                <form class="d-flex" role="search">
                    <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                    <button class="btn btn-outline-primary" type="submit">Search</button>
                </form>
            </div>
        </div>
    </nav>
    <div class="my-3 p-3 bg-body rounded shadow-sm">
        <h6 class="border-bottom pb-2 mb-0">最新议题</h6>
        <% for(var i = 0; i < drafts.length; i++) { %>
            <div class="d-flex text-muted pt-3">
                <img class="bd-placeholder-img flex-shrink-0 me-2 rounded" src="/images/3.svg" alt="" width="32" height="32">
                <div class="pb-3 mb-0 small lh-sm border-bottom w-100">
                    <div class="d-flex justify-content-between">
                        <strong class="text-gray-dark"><%= drafts[i].draftName %></strong>
                        <a href="/vote/vote-detail?vid=<%= drafts[i].draftId %>" id="invalid">Follow</a>
                    </div>
<!--                    <span class="d-block"><em>创建者：<%= drafts[i].address%></em></span>-->
                    <span class="d-block"><%= drafts[i].topic %></span>
                    <span class="d-block"><em>创建者：<%= drafts[i].address%></em></span>
                </div>
            </div>
        <% } %>
